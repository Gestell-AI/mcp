{
  "name": "@gestell/mcp",
  "version": "0.2.0",
  "module": "dist/library.js",
  "type": "module",
  "scripts": {
    "start:terminal": "bun run server/terminal.ts",
    "start:remote": "bun run server/remote.ts",
    "lint": "eslint .",
    "build": "bun run build:lib && bun run build:cli",
    "build:lib": "bun build server/library.ts --target node --outfile ./dist/library.js",
    "build:cli": "bun build server/entry.ts --target node --outfile ./dist/entry.js"
  },
  "bin": {
    "gestell-mcp": "dist/entry.js"
  },
  "dependencies": {
    "@gestell/sdk": "1.3.0",
    "@modelcontextprotocol/sdk": "1.11.0",
    "dotenv": "16.5.0",
    "fastify": "5.3.2",
    "pino-pretty": "13.0.0",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@ai-sdk/google": "1.2.17",
    "@eslint/js": "9.26.0",
    "@types/bun": "1.2.12",
    "ai": "4.3.15",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.3",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jiti": "2.4.2",
    "typescript-eslint": "8.32.0"
  },
  "peerDependencies": {
    "typescript": "5.8.3"
  }
}
